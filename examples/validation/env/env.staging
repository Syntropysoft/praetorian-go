API_BASE_URL=https://staging-api.example.com
API_TIMEOUT=8000
API_RETRIES=3

LOG_LEVEL=warn
LOG_FORMAT=json
LOG_OUTPUT=console

MONITORING_ENABLED=true
METRICS_PORT=9090
METRICS_PATH=/metrics 