# Staging Environment Configuration
APP_NAME=my-app
APP_PORT=8080
APP_ENVIRONMENT=staging

DB_HOST=staging-db.example.com
DB_PORT=5432
DB_NAME=staging_db
DB_USER=${DB_USER}
